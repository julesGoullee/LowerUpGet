{
  "name": "lower-up-get",
  "version": "1.0.0",
  "description": "Get simple page and return simple and fast content",
  "engines": {
    "node": "4.2.6",
    "npm": "2.14.7"
  },
  "scripts": {
    "api:start": "node services/api/api.js",
    "page-executor:start": "node services/pageExecutor/singleInstance.js",
    "pm2:start": "npm run logs:clean && pm2 start config/pm2-app.json",
    "logs:clean": "rm -rf ./logs/**",
    "lint": "eslint .",
    "test": "mocha -R spec ./test/unit/test.config.js ./test/unit/**/*.js --timeout 5000",
    "travis": "npm run lint && npm test"
  },
  "keywords": [
    "web",
    "page"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/julesGoullee/LowerUpGet.git"
  },
  "author": "Jules Goull√©e",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.2.2",
    "forever": "^0.15.1",
    "jsdom": "^8.0.2",
    "pm2": "^1.0.0",
    "request": "^2.69.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^1.10.3",
    "mocha": "^2.4.5",
    "mock-require": "^1.2.1"
  }
}
